    "form-data": "^3.0.4",
    "mailgun.js": "^12.1.1"
  },
  "devDependencies": {
    "chai": "^6.2.0",
    "firebase-functions-test": "^3.1.0",
    "mocha": "^11.7.5",
    "sinon": "^21.0.0"
  },
  "private": true
}
